{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "nuxtjs: debug",
			"type": "npm",
			"script": "dev",
			"isBackground": true,
			"detail": "Runs nuxt with debugging enabled",
			"problemMatcher": [{
				"base": "$tsc-watch",
				"background": {
					"activeOnStart": true,
					"beginsPattern": "Starting development server",
					"endsPattern": "Compiled successfully"
				}
			}],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}